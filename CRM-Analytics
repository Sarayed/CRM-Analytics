     "shell.execute_reply": "2024-05-05T12:11:18.640781Z"
    },
    "papermill": {
     "duration": 0.117592,
     "end_time": "2024-05-05T12:11:18.644523",
     "exception": false,
     "start_time": "2024-05-05T12:11:18.526931",
     "status": "completed"
