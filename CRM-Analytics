     "execution_count": 18,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import datetime as dt\n",
    "\n",
    "# Ensure the 'TotalPrice' column exists by calculating it from 'Quantity' and 'Price'\n",
    "df['TotalPrice'] = df['Quantity'] * df['Price']\n",
    "\n",
    "def create_rfm(dataframe):\n",
    "    \"\"\"\n",
    "    Create RFM analysis to identify customer segments based on Recency, Frequency, and Monetary values.\n",
    "    \n",
    "    Parameters:\n",
    "    dataframe (pd.DataFrame): The input dataset containing customer transaction data.\n",
    "    \n",
    "    Returns:\n",
    "    pd.DataFrame: A dataframe with calculated RFM metrics and segments.\n",
    "    \"\"\"\n",
    "    # Define the current date for calculating recency\n",
    "    today_date = dt.datetime(2010, 12, 11)\n",
    "\n",
    "    # Group by Customer ID and calculate Recency, Frequency, and Monetary\n",
    "    rfm = dataframe.groupby('Customer ID').agg({\n",
    "        'InvoiceDate': lambda date: (today_date - date.max()).days,  # Recency (in days)\n",
    "        'Invoice': 'nunique',  # Frequency (unique invoices)\n",
    "        'TotalPrice': 'sum'  # Monetary (total spent)\n",
    "    })\n",
    "\n",
    "    # Rename the aggregated columns for clarity\n",
    "    rfm.columns = ['recency', 'frequency', 'monetary']\n",
    "\n",
    "    # Remove customers with negative or zero monetary value\n",
    "    rfm = rfm[rfm['monetary'] > 0]\n",
    "\n",
    "    # Assign recency, frequency, and monetary scores using quintiles\n",
    "    rfm['recency_score'] = pd.qcut(rfm['recency'], 5, labels=[5, 4, 3, 2, 1])  # More recent, higher score\n",
    "    rfm['frequency_score'] = pd.qcut(rfm['frequency'].rank(method='first'), 5, labels=[1, 2, 3, 4, 5])  # More frequent, higher score\n",
    "    rfm['monetary_score'] = pd.qcut(rfm['monetary'], 5, labels=[1, 2, 3, 4, 5])  # Higher monetary value, higher score\n",
    "\n",
    "    # Create RFM segments by combining recency and frequency scores\n",
    "    rfm['rfm_segment'] = rfm['recency_score'].astype(str) + rfm['frequency_score'].astype(str)\n",
    "\n",
    "    # Define mapping for segment descriptions\n",
    "    seg_map = {\n",
    "        r'[1-2][1-2]': 'hibernating',\n",
    "        r'[1-2][3-4]': 'at_risk',\n",
    "        r'[1-2]5': 'can_t_lose',\n",
    "        r'3[1-2]': 'about_to_sleep',\n",
    "        r'33': 'need_attention',\n",
    "        r'[3-4][4-5]': 'loyal_customers',\n",
    "        r'41': 'promising',\n",
    "        r'51': 'new_customers',\n",
    "        r'[4-5][2-3]': 'potential_loyalists',\n",
    "        r'5[4-5]': 'champions'\n",
    "    }\n",
    "\n",
    "    # Apply the mapping to generate meaningful segment names\n",
    "    rfm['rfm_segment'] = rfm['rfm_segment'].replace(seg_map, regex=True)\n",
    "\n",
    "    # Select only the necessary columns\n",
    "    rfm = rfm[['recency', 'frequency', 'monetary', 'rfm_segment']]\n",
    "\n",
    "    return rfm\n",
    "\n",
    "\n",
    "# Apply the RFM analysis to your dataframe\n",
    "rfm = create_rfm(df)\n",
    "\n",
    "# Display the first few rows of the RFM table\n",
    "rfm.head()\n"
   ]
  },
