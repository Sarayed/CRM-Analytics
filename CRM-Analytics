    "import seaborn as sns\n",
    "\n",
    "# Group by 'cltv_predicted_segment' and calculate the mean of 'expected_average_profit'\n",
    "avg_profit_by_segment = rfm_cltv_predicted.groupby('cltv_predicted_segment')['expected_average_profit'].mean()\n",
    "\n",
    "# Create a bar plot to visualize the average expected profit for each CLTV segment\n",
