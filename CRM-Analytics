   "source": [
    "# Ensure seaborn styling is applied\n",
    "sns.set(style=\"whitegrid\")\n",
    "\n",
    "# Histogram of the predicted CLTV values\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.histplot(rfm_cltv_predicted[\"cltv_predicted\"], kde=True)\n",
