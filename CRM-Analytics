   "cell_type": "code",
   "execution_count": 29,
   "id": "ae4eae16",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-05-05T12:11:01.042003Z",
     "iopub.status.busy": "2024-05-05T12:11:01.041633Z",
     "iopub.status.idle": "2024-05-05T12:11:01.183048Z",
     "shell.execute_reply": "2024-05-05T12:11:01.181969Z"
    },
    "papermill": {
     "duration": 0.170156,
     "end_time": "2024-05-05T12:11:01.185886",
     "exception": false,
     "start_time": "2024-05-05T12:11:01.015730",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.preprocessing import MinMaxScaler\n",
    "\n",
    "def create_cltv_calculate(dataframe):\n",
    "    \"\"\"\n",
    "    Calculate Customer Lifetime Value (CLTV) and related metrics from RFM data.\n",
    "    \n",
    "    Parameters:\n",
    "    dataframe (pd.DataFrame): DataFrame with customer data, including recency, frequency, and monetary.\n",
    "    \n",
    "    Returns:\n",
    "    pd.DataFrame: DataFrame with CLTV calculations and segments.\n",
    "    \"\"\"\n",
    "    # Average Order Value\n",
    "    dataframe['avg_order_value'] = dataframe['monetary'] / dataframe['frequency']\n",
    "\n",
    "    # Purchase Frequency\n",
    "    # Check for possible division by zero and set it to NaN\n",
    "    dataframe['purchase_frequency'] = dataframe['frequency'] / dataframe.shape[0]\n",
    "\n",
    "    # Repeat Rate and Churn Rate\n",
    "    repeat_rate = dataframe[dataframe['frequency'] > 1].shape[0] / dataframe.shape[0]\n",
    "    churn_rate = 1 - repeat_rate\n",
    "\n",
    "    # Profit Margin (5% assumed)\n",
    "    dataframe['profit_margin'] = dataframe['monetary'] * 0.05\n",
    "\n",
    "    # Customer Value\n",
    "    dataframe['customer_value'] = dataframe['avg_order_value'] * dataframe['purchase_frequency']\n",
    "\n",
    "    # Customer Lifetime Value (CLTV)\n",
    "    dataframe['cltv'] = (dataframe['customer_value'] / churn_rate) * dataframe['profit_margin']\n",
    "\n",
    "    # Scale CLTV for consistent range (1 to 100)\n",
    "    scaler = MinMaxScaler(feature_range=(1, 100))\n",
    "    dataframe['cltv_calculated'] = scaler.fit_transform(dataframe[['cltv']])\n",
    "\n",
    "    # Segmenting CLTV into three categories\n",
    "    dataframe['cltv_segment'] = pd.qcut(dataframe['cltv_calculated'], 3, labels=[\"Low\", \"Medium\", \"High\"])\n",
    "\n",
    "    # Keep relevant columns only\n",
    "    return dataframe[['recency', 'frequency', 'monetary', 'rfm_segment', 'cltv_calculated', 'cltv_segment']]\n"
   ]
  },
